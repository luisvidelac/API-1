use pjud;

/* causaciviles */

db.getCollection('causaciviles').createIndex(
  {
    "Rol" : 1,
    "Caratulado" : 1,
    "Tribunal" : 1,
    "FechaEstDia": 1,
    "usuarios": 1
},
   {
     name: "find_causaciviles"
   }
);

db.getCollection('causaciviles').createIndex(
  {
    "updated_at" : 1
},
   {
     name: "updated_at_causaciviles"
   }
);

